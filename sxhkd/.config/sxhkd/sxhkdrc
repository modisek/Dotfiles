# terminal emulator
shift + Return
 st

super + z
  gt
#pcmanfm
ctrl + shift + f
  st -e ranger $HOME

#alsamixer
ctrl + shift + s
  st -e alsamixer

# make sxhkd reload its configuration files:
super + Escape
  pkill -USR1 -x sxhkd

ctrl + b
  firefox

ctrl + w
  ~/basilisk/basilisk

XF86AudioRaiseVolume
  pactl set-sink-volume @DEFAULT_SINK@ +5%

XF86AudioLowerVolume
  pactl set-sink-volume @DEFAULT_SINK@ -5%

XF86AudioMute
  pactl set-sink-mute @DEFAULT_SINK@ toggle

XF86AudioMicMute
  pactl set-source-mute @DEFAULT_SOURCE@ toggle

XF86AudioPlay
  playerctl play-pause

XF86AudioPause
  playerctl pause


XF86AudioNext 
  playerctl next

XF86AudioPrev 
  playerctl previous
 
ctrl +  XF86AudioPlay
  mpc toggle

ctrl + XF86AudioNext 
  mpc next

ctrl + XF86AudioPrev 
  mpc prev

XF86Forward
  mpc seek +5

XF86Back
  mpc seek -5

Print
 scrot '%Y-%m-%d_%H-%M-%S.png' -e  'mv $f ~/Pictures/Screenshots/'


ctrl + t
  trayer

ctrl + shift + t
  pkill trayer
  
XF86MonBrightnessUp
    light -A 10

XF86MonBrightnessDown
    light -U 10

ctrl + z
   xcolor |xclip -sel clip
